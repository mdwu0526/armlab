<?xml version='1.0'?>
<sdf version="1.4" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:arg name="uri" default="" />
  <model name="robotApriltag">
    <pose>0 0 0 0 0 0</pose>
    <static>false</static>
    <link name="link">
      <gravity>0</gravity>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>$(arg uri)</uri>
            <scale>0.001 0.001 0.001</scale>
          </mesh>
        </geometry>
      </visual>
    </link>
  </model>
</sdf>
